      </servlet>
      <servlet>
     	<servlet-name>DisplayCart</servlet-name>
      	<display-name>DisplayCart</display-name>
      	<description></description>
      	<servlet-class>myfiles.DisplayCart</servlet-class>
      </servlet>
      <servlet-mapping>
      	<servlet-name>CartServlet</servlet-name>
      	<url-pattern>/CartServlet</url-pattern>
      </servlet-mapping>
      <servlet-mapping>
      	<servlet-name>DisplayCart</servlet-name>
      	<url-pattern>/DisplayCart</url-pattern>
     </servlet-mapping>
    </web-app>
    